{
  "name": "netlify-plugin-example",
  "version": "0.0.1",
  "description": "Netlify Build plugin - {{description}}",
  "main": "src/main.js",
  "files": [
    "src",
    "!*~"
  ],
  "scripts": {
    "init": "npm install --loglevel error --no-audit --no-fund && node init/bin.js",
    "build": "netlify-build",
    "test": "npm run lint && npm run ava",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ignore-path .gitignore --fix --cache --format=codeframe --max-warnings=0 \"{src,init}/**/*.js\"",
    "prettier": "prettier --ignore-path .gitignore --write --loglevel warn \"{.github,src,init}/**/*.{js,md,yml,json}\" \"*.{js,md,yml,json}\"",
    "ava": "ava --verbose",
    "release": "release-it"
  },
  "keywords": [
    "netlify-plugin",
    "netlify"
  ],
  "author": "{{author}} <{{email}}>",
  "license": "{{license}}",
  "repository": "{{repo}}",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "@netlify/build": "^1.0.6",
    "ava": "^2.4.0",
    "chalk": "^3.0.0",
    "cpy": "^8.1.0",
    "del": "^5.1.0",
    "email-validator": "^2.0.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "execa": "^4.0.0",
    "fast-glob": "^3.2.2",
    "filter-obj": "^2.0.1",
    "fuzzy": "^0.1.3",
    "git-remote-origin-url": "^3.1.0",
    "inquirer": "^7.1.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "netlify-cli": "^2.40.0",
    "omit.js": "^1.0.2",
    "prettier": "^1.19.1",
    "release-it": "^13.5.1",
    "semver": "^7.1.3",
    "spdx-license-list": "^6.1.0",
    "strip-ansi": "^6.0.0",
    "username": "^5.1.0",
    "yargs": "^15.3.1"
  },
  "engines": {
    "node": ">=8.3.0"
  }
}
